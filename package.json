{
  "name": "pokedex-app",
  "version": "1.0.0",
  "description": "Pokedex App via React.js",
  "main": "index.js",
  "scripts": {
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "node server.js",
    "client": "cd client && npm start",
    "serve": "nodemon server.js",
    "dev": "concurrently \" npm run serve\" \"npm --prefix ./client run start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasyurek/pokedex-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tasyurek/pokedex-app/issues"
  },
  "homepage": "https://github.com/tasyurek/pokedex-app#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.4"
  }
}
